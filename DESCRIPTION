Package: soilfoodwebs
Type: Package
Title: Soil Food Web Analysis
Version: 0.1.0
Authors@R: c(
person("Robert", "Buchkowski", email = "robert.buchkowski@gmail.com",
  role = c("aut", "cre", "cph")),
